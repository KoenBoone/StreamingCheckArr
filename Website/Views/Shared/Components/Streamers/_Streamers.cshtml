@model List<string>
aaa
<span>
    @{
        //if the model is not empty
        if (Model.Count > 0)
        {
            //loop through the model
            foreach (var item in Model)
            {
                //display the image
                <img src="@item" class="img-rounded img-size-64 mr-2" />
            }
        }
        else
        {
            @Html.ActionLink("Check streaming", "GetStreamers", new { tmdbID = ViewBag.tmdbID })

        }}
</span>